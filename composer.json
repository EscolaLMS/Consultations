{
  "name": "escolalms/consultations",
  "description": "Escola Headless LMS Consultations",
  "type": "package",
  "require": {
    "php": ">=7.4",
    "escolalms/auth": "^0",
    "escolalms/categories": "^0",
    "escolalms/core": "^1",
    "escolalms/jitsi": "^0",
    "escolalms/settings": "^0",
    "escolalms/model-fields": "^0",
    "laravel/framework": ">=9.0",
    "escolalms/files": "^0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^7.0",
    "nunomaduro/larastan": "^2.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Hubert Krzysztofiak",
      "email": "hubert.krzysztofiak@escolasoft.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\Consultations\\": "src",
      "EscolaLms\\Consultations\\Tests\\": "tests",
      "EscolaLms\\Consultations\\Database\\Factories\\": "database/factories",
      "EscolaLms\\Consultations\\Database\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\Consultations\\EscolaLmsConsultationsServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
