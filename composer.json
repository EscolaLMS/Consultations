{
  "name": "escolalms/consultations",
  "description": "Escola Headless LMS Consultations",
  "type": "package",
  "require": {
    "php": ">=7.4",
    "davidbadura/faker-markdown-generator": "^1.1",
    "escolalms/core": "^1",
    "escolalms/auth": "^0",
    "escolalms/settings": "^0",
    "escolalms/jitsi": "^0",
    "laravel/framework": ">=8.0",
    "escolalms/cart": "0.1.19"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": ">=5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Hubert Krzysztofiak",
      "email": "hubert.krzysztofiak@escolasoft.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\Consultations\\": "src",
      "EscolaLms\\Consultations\\Tests\\": "tests",
      "EscolaLms\\Consultations\\Database\\Factories\\": "database/factories",
      "EscolaLms\\Consultations\\Database\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\Consultations\\EscolaLmsConsultationsServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
